{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "array-type": [true, "array"],
    "object-literal-sort-keys": false,
    "interface-name": false,
    "no-console": false,
    "member-access": false,
    "max-classes-per-file": false
  }
}
