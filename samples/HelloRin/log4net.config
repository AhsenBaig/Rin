<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <log4net>
    <appender name="Console" type="log4net.Appender.ManagedColoredConsoleAppender"></appender>
    <appender name="Rin" type="Rin.Log4NetAppender.RinLogAppender, Rin.Log4NetAppender"></appender>
    <root>
      <level value="All" />
      <appender-ref ref="Console" />
      <appender-ref ref="Rin" />
    </root>
  </log4net>
</configuration>
